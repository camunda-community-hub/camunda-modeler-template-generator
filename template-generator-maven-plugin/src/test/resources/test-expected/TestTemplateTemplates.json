[
  {
    "$schema": "https://unpkg.com/@camunda/element-templates-json-schema@0.12.0/resources/schema.json",
    "name": "Example",
    "id": "com.example.Example",
    "description": "Example of a Template",
    "documentationRef": "https://docs.camunda.io",
    "appliesTo": [
      "bpmn:Task"
    ],
    "entriesVisible": false,
    "properties": [
      {
        "label": "Example Additional Property",
        "type": "String",
        "value": "example",
        "description": "Example of an additional property",
        "documentationRef": "https://docs.camunda.io",
        "editable": false,
        "constraints": {
          "notEmpty": true
        },
        "binding": {
          "type": "camunda:inputParameter",
          "name": "exampleAdditionalProperty"
        }
      },
      {
        "type": "Hidden",
        "value": "exampleFunction",
        "editable": false,
        "binding": {
          "type": "camunda:inputParameter",
          "name": "exampleNameProperty"
        }
      },
      {
        "type": "Hidden",
        "value": "org.camunda.community.template.generator.test.template.TestTemplate",
        "editable": false,
        "binding": {
          "type": "property",
          "name": "camunda:class"
        }
      }
    ]
  }
]
